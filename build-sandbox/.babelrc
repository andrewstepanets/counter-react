{
  "presets": [
    [
      "@babel/preset-env",
      {
        "debug": true,
        "targets": [
          // last 2 version browsers
          //   "last 2 chrome versions",
          //   "last 2 firefox versions",
          //   "last 2 ios versions"
          // browser supporting more than 0.3% of people not IE
          "> 0.3%",
          "not ie > 0"
        ]
      }
    ]
  ],
  "plugins": ["@babel/proposal-class-properties"]
}
